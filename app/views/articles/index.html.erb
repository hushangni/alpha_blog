<h1>Articles Listing page</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Descriptions</th>
            <th>Actions</th>
        </tr>
    </thead>

    <tbody>
        <% @articles.each do |article| %>
        <tr>
            <td><%= article.title =%></td>
            <td><%= article.description =%></td>
            <td><%= article.created_at =%></td>
        </tr>
        <% end %>
    </tbody>
</table>
